{
  "expo": {
    "name": "WheelX",
    "slug": "mobile",
    "scheme": "wheelx",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.wheelx.app"
    },
    "android": {
      "package": "com.wheelx.app",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "2441db3b-6dee-46fe-8408-5f0811c4f345"
      }
    },
    "owner": "apurvabera",
    "plugins": [
      "expo-web-browser"
    ]
  }
}